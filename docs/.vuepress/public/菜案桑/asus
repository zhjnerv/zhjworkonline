proxy-providers:

  openitsub_ml:
    type: http
    interval: 3600
    health-check:
      enable: true
      url: http://www.gstatic.com/generate_204
      interval: 1000
    url: "https://raw.githubusercontent.com/yu-steven/openit/main/Clash.yaml"
    path: ./proxy_providers/openitsub_ml.yaml

  mylinks:
    type: http
    interval: 1800
    health-check:
