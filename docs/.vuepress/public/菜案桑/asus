#分隔，下面是地区分组
  - name: 美国
    filter: "美|美国|US|UnitedStates|us"
    type: select
    use:
      - openitsub_ml
      - mylinks
      - somelinks
      - 蛋黄的连接
      - paimon

  - name: 其它地区
    filter: "^(?!.*(?:🇺🇸|美国|US)).*"
    type: select
    use:
      - openitsub_ml
      - mylinks
      - somelinks
      - 蛋黄的连接
      - paimon

  - name: 全部
    type: select
    use:
      - openitsub_ml
      - mylinks
      - somelinks
      - 蛋黄的连接
      - paimon
  - name: 自动选择
    use:
      - openitsub_ml
      - mylinks
      - somelinks
      - 蛋黄的连接
      - paimon
    type: url-test
    url: 'http://www.gstatic.com/generate_204'
    interval: 300
    tolerance: 10

