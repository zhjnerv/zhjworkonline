proxy-groups:

# 功能分组
  
  - name: 🌏️ 节点选择
    type: select
    proxies:
      - hy2-us-zjh
      - 🚀 自动选择
      - 🔄 手动切换
      - ✅ 解锁 AI
      - DIRECT
      - 📊 测试之选
      - 🔁 故障转移

  - name: 📢 电报
    type: select
    proxies:
      - hy2-us-zjh
      - 🚀 自动选择
      - 🔄 手动切换
      - ✅ 解锁 AI
      - DIRECT
      - 📊 测试之选
      - 🔁 故障转移
    use: 
      - Getafreenode

  - name: ↪️ 漏网之鱼
    type: select
    proxies:
      - hy2-us-zjh
      - 🚀 自动选择
      - 🔄 手动切换
      - ✅ 解锁 AI
      - DIRECT
      - 📊 测试之选
      - 🔁 故障转移


  - name: 🚀 自动选择
    <<: *all 
    type: url-test
    url: "https://www.google.com/generate_204"
    interval: 300
    tolerance: 50
    lazy: true
    filter: "(?i)^(?!unavailable).*$"
    
  - name: 🔄 手动切换
    <<: *all
    filter: "(?i)^(?!unavailable).*$"

  - name: ✅ 解锁 AI
    <<: *ai
    proxies:
      - hy2-us-zjh 
      - 📊 测试之选
    
  - name: ☑️ 解锁 Gemini 
    <<: *all
    filter: "(?i)(us|unitedstates|united states|usa|jp|sg|zgo|tw|sjc|中转 US|中转 TW|中转 JP|GPT|解锁)(?i)(?!.*(hong kong|hk|香港|香|港|ccs))"


  - name: 📊 测试之选
    type: select
    use:
      - mylocal
      - 星链
    filter: "(?i)^(?!unavailable).*$"

  - name: 🔁 故障转移
    type: fallback
    url: "https://www.google.com/generate_204"
    interval: 60
    <<: *all
