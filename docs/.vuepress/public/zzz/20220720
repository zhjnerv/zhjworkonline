    - tls://[2606:4700:4700::1111]:853 # Cloudflare IPv6 DNS (TLS)
    - tls://[2001:4860:4860::8888]:853 # Google IPv6 DNS (TLS)
  fallback-filter:
    geoip: true
    geoip-code: CN
    ipcidr:
      - 240.0.0.0/4
  enhanced-mode: redir-host
