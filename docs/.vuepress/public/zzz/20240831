    <<: *p
    url: ""
    
  订阅 8: # 
    <<: *p
    url: ""

proxy-groups:

# 功能分组
  
  - name: 🌏️ 节点选择
    type: select
    proxies:
      - 🚀 自动选择
      - 🔄 手动切换
      - ✅ 解锁 AI
      - ⬇ 精选节点
      - ⬇ 备用应急
      - DIRECT
      
  - name: 🚀 自动选择
    <<: *fly
    filter: "(?i)^(?!unavailable).*$"
    
  - name: 🔄 手动切换
    <<: *all
    filter: "(?i)^(?!unavailable).*$"
    
  - name: ✅ 解锁 AI
    <<: *ai

  - name: ⬇ 精选节点
    <<: *best

  - name: ⬇ 备用应急
    <<: *backup
    filter: "(?i)^(?!unavailable).*$"

  - name: ↪️ 漏网之鱼
    type: select
    proxies:
      - 🌏️ 节点选择
      - DIRECT

