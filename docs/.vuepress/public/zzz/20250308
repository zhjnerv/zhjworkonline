proxy-groups:

# 功能分组
  
  - name: 🌏️ 节点选择
    type: select
    proxies:
      - 🚀 自动选择
      - 🔄 手动切换
      - ✅ 解锁 AI
      - ⬇ 精选节点
      - ⬇ 备用应急
      - DIRECT
      - 📊 测试之选
      - 💟 核心之选
      - 🔁 故障转移

  - name: 📢 电报
    type: select
    proxies:
      - 🚀 自动选择
      - 🔄 手动切换
      - ✅ 解锁 AI
      - ⬇ 精选节点
      - ⬇ 备用应急
      - DIRECT
      - 📊 测试之选
      - 💟 核心之选
      - 🔁 故障转移
  - name: ↪️ 漏网之鱼
    type: select
    proxies:
      - 🚀 自动选择
      - 🔄 手动切换
      - ✅ 解锁 AI
      - ⬇ 精选节点
      - ⬇ 备用应急
      - DIRECT
      - 📊 测试之选
      - 💟 核心之选
      - 🔁 故障转移



  - name: 🚀 自动选择
    <<: *fly
    filter: "(?i)^(?!unavailable).*$"
    
  - name: 🔄 手动切换
    <<: *all
    filter: "(?i)^(?!unavailable).*$"
    
  - name: ✅ 解锁 AI
    <<: *ai

  - name: ⬇ 精选节点
    <<: *best

  - name: ⬇ 备用应急
    <<: *backup
    filter: "(?i)^(?!unavailable).*$"


  - name: 💟 核心之选
    type: select
    use:
      - somelinks
      - 竹影
      - mylocal
      - 宝可梦星云

  - name: 📊 测试之选
    type: select
    use:
      - 测试1-OP-API
      - Linuxdo冷门节点
      - mylocal
      - 宝可梦星云
    filter: "(?i)^(?!unavailable).*$"


  - name: 🔁 故障转移
    type: fallback
    url: "http://www.google.com/generate_204"
    interval: 300
    tolerance: 50
    use:
      - 测试1-OP-API
      - Linuxdo冷门节点
      - 神奇的订阅8
      - Linuxdo好人2
      - Linuxdo好人
      - colo优选
