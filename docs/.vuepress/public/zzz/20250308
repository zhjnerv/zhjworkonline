proxy-groups:

# 功能分组
  
  - name: 🌏️ 节点选择
    type: select
    proxies:
      - 🚀 自动选择
      - 🔄 手动切换
      - ✅ 解锁 AI
      - ⬇ 精选节点
      - ⬇ 备用应急
      - DIRECT
      - 📊 测试之选

      - 🔁 故障转移
      - 🔵 负载均衡


  - name: 📢 电报
    type: select
    proxies:
      - 🚀 自动选择
      - 🔄 手动切换
      - ✅ 解锁 AI
      - ⬇ 精选节点
      - ⬇ 备用应急
      - DIRECT
      - 📊 测试之选

      - 🔁 故障转移
    use: 
      - Getafreenode

  - name: ↪️ 漏网之鱼
    type: select
    proxies:
      - 🚀 自动选择
      - 🔄 手动切换
      - ✅ 解锁 AI
      - ⬇ 精选节点
      - ⬇ 备用应急
      - DIRECT
      - 📊 测试之选

      - 🔁 故障转移
      - 🔵 负载均衡



  - name: 🚀 自动选择
    <<: *fly
    filter: "(?i)^(?!unavailable).*$"
    
  - name: 🔄 手动切换
    <<: *all
    filter: "(?i)^(?!unavailable).*$"

  - name: ✅ 解锁 AI
    <<: *ai
    proxies: 
      - 📊 测试之选
    
  - name: ☑️ 解锁 Gemini 
    <<: *all
    filter: "(?i)(us|unitedstates|united states|usa|jp|sg|zgo|tw|sjc|中转 US|中转 TW|中转 JP|GPT|解锁)(?i)(?!.*(hong kong|hk|香港|香|港|ccs))"


  - name: ⬇ 精选节点
    <<: *best

  - name: ⬇ 备用应急
    <<: *backup
    filter: "(?i)^(?!unavailable).*$"



  - name: 📊 测试之选
    type: select
    use:
      - 测试1-OP-API
      - Linuxdo冷门节点
      - mylocal
      - 神奇的订阅9
      - 长期有效自动更新
    filter: "(?i)^(?!unavailable).*$"

  - name: 🔵 负载均衡
    type: load-balance
    strategy: consistent-hashing
    url: https://cp.cloudflare.com/generate_204
    interval: 300
    tolerance: 100
    <<: *all

  - name: 🔁 故障转移
    type: fallback
    url: "https://www.google.com/generate_204"
    interval: 60
    <<: *all
